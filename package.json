{
  "name": "server",
  "version": "1.0.0",
  "description": "backend of real-estate",
  "main": "src/index.ts",
  "scripts": {
    "build": "rimraf dist &&  npx tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "npm run build &&  concurrently \"npx tsc -w\" \"nodemon  --exec ts-node -r tsconfig-paths/register src/index.ts\" ",
    "db:generate": "npx drizzle-kit generate",
    "db:reset": "ts-node -r tsconfig-paths/register src/scripts/reset-db.ts",
    "db:push": "npx drizzle-kit push",
    "postdb:push": "ts-node -r tsconfig-paths/register src/scripts/configuration-db.ts"
  },
  "keywords": [
    "rent",
    "nodejs",
    "nextJs",
    "api",
    "house"
  ],
  "author": "Anis Larguet",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.804.0",
    "@aws-sdk/lib-storage": "^3.804.0",
    "@terraformer/wkt": "^2.2.1",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "pg": "^8.15.6",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.14",
    "@types/pg": "^8.15.0",
    "@types/terraformer__wkt": "^2.0.3",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.31.1",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
